    <event eventtype="3" enumb="0">
      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///BOSS

switch machine.game_boss
{   case 2: if !wobb for (var i=0;i&lt;=2;i++)
            {   switch i
                {   case 0: var j = lgrip;
                            var k = !wobb;
                            break;
                    case 1: j = rgrip;
                            k = !wobb;
                            break;
                    case 2: j = id;
                            k = !(lgrip.wobb and rgrip.wobb);
                            break;
                }
                with j if k
                {   y += 50 * sign(room_height*(0.15+0.6*other.placement) - y);
                    if i != 2
                    {   if other.placement
                        {   if y &gt;= room_height*(0.15+0.6*other.placement)
                                wobb = 1;
                        }
                        else if y &lt;= room_height*(0.15+0.6*other.placement)
                            wobb = 1;
                    }
                    else if attack
                        with instance_create(x,y,bullet) 
                            motion_add(other.a_shoot,80);
                }
                else if i == 2
                    wobb = 1;
            }
            else
            {   if !timer1 and !timer2
                {   wobb++;
                    if instance_exists(machine.me)
                        a_shoot = 20*(1 + min((a_angle-90)/90,1) - max((a_angle-90)/90,1))
                                + point_direction(x,y,machine.me.x,machine.me.y);
                    a_angle += 3;
                    if a_angle &gt;= 360
                        a_angle -= 360;
                    if !(wobb%(room_speed/3))
                    {   with lgrip y = clamp(y+irandom_range(-15,15),
                                       room_height*(0.1+0.6*other.placement),
                                       room_height*(0.2+0.6*other.placement));
                        with rgrip y = clamp(y+irandom_range(-15,15),
                                       room_height*(0.1+0.6*other.placement),
                                       room_height*(0.2+0.6*other.placement));
                    }
                    if !(wobb%room_speed)
                    {   if random(1) &lt; damage/6
                        with instance_create(irandom_range(room_width*0.2,room_width*0.8),
                                             room_height*-0.2,waste)
                        {   image_xscale = 3;
                            image_yscale = 3;
                            gravity = 0.05;
                            shot = 0;
                            with instance_create(x,0,warning)
                            {   timer = 2;
                                image_blend = c_green;
                            }
                        }
                        if irandom(100) &lt; wobb/room_speed
                            timer1 = 1;
                        else if irandom(100) &lt; 40
                        {   timer1 = 1;
                            attack = 3;
                            action = 1;
                        }
                        else if !placement and irandom(100) &lt; 8
                        {   timer1 = 0.8;
                            attack = 4;
                            action = 2;
                            att = ds_queue_create();
                        }
                    }
                }
                else switch action
                {   case 0: if timer2
                            {   timer2 -= 1/room_speed;
                                if !timer2
                                {   wobb = 0;
                                    lgrip.wobb = 0;
                                    rgrip.wobb = 0;
                                    placement ^= 1;
                                    attack = 1;
                                }
                            }
                            else
                            {   if timer1 == 1
                                    timer1--;
                                timer1 += 720/room_speed;
                                a_shoot = timer1;
                                if !(timer1%180) and (irandom(100)&lt;timer1/90)
                                {   timer1 = 0;
                                    timer2 = 2;
                                }
                            }
                            break;
                    case 1: if timer2
                            {   timer2 -= 1/room_speed;
                                if !timer2
                                {   attack--;
                                    with instance_create(x,y,bullet)
                                        motion_add(other.a_shoot,80);
                                    if attack
                                        timer2 = 0.7;
                                    else
                                        action = 0;
                                }
                            }
                            else
                            {   timer1 -= 1/room_speed;
                                if !timer1
                                    timer2 = 0.7;
                            }
                            break;
                    case 2: if timer2
                            {   timer2 -= 1/room_speed;
                                if !timer2
                                {   var a = ds_queue_dequeue(att);
                                    a_shoot = 270;
                                    with instance_create(x,y,bullet)
                                        motion_add(point_direction(x,y,a[0],a[1]),80);
                                    if !ds_queue_empty(att)
                                        timer2 = 1;
                                    else
                                    {   action = 0;
                                        ds_queue_destroy(att);
                                    }
                                }
                            }
                            else
                            {   timer1 -= 1/room_speed;
                                if !timer1
                                {   attack--;
                                    var a;
                                    a[1] = irandom_range(room_height*0.3,room_height*0.6);
                                    a[0] = irandom_range(room_width*0.1,room_width*0.9);
                                    ds_queue_enqueue(att,a);
                                    a_shoot = point_direction(x,y,a[0],a[1]);
                                    if attack
                                        timer1 = 0.8;
                                    else
                                        timer2 = 1;
                                }
                            }
                            break;
                }
            }
            //var aspd = 0.012*point_distance(room_width/2,placement,x,y);
            //var adir = point_direction(room_width/2,placement,x,y);
            var aspd1 = 0.011*point_distance(lgrip.x,lgrip.y,x,y);
            var adir1 = point_direction(lgrip.x,lgrip.y,x,y);
            var aspd2 = 0.009*point_distance(rgrip.x,rgrip.y,x,y);
            var adir2 = point_direction(rgrip.x,rgrip.y,x,y);
            //hspeed -= (irandom_range(-1,1)+aspd) * dcos(adir) * 0.8;
            //vspeed += (irandom_range(-1,1)+aspd) * dsin(adir) * 0.8;
            hspeed -= /*aspd*dcos(adir) +*/ aspd1*dcos(adir1) + aspd2*dcos(adir2);
            vspeed += /*aspd*dsin(adir) +*/ aspd1*dsin(adir1) + aspd2*dsin(adir2);
            if damage &gt;= 1
            {   damage = 0;
                hspeed = 0;
                vspeed = 0;
                machine.game_boss++;
                with rgrip instance_destroy();
            }
            //vspeed -= 0.2;
            //lgrip.vspeed -= 0.2;
            //rgrip.vspeed -= 0.2;
            break;
    case 3: wobb++;
            if !(wobb%room_speed)
            {   with instance_create(irandom_range(room_width*0.05,room_width*0.95),
                                     room_height*-0.2,waste)
                {   image_xscale = 3;
                    image_yscale = 3;
                    gravity = 0.05;
                    shot = 0;
                    with instance_create(x,0,warning)
                    {   timer = 2;
                        image_blend = c_green;
                    }
                }
            }
            if damage &gt;= 1
            {   machine.game_boss++;
                wobb = 0;
                placement = 0;
                damage = 0;
                action = 0;
                attack = 0;
                y = lgrip.y;
            }
            break;
    case 4: if !timer1 and !timer2
            {   x = room_width*(0.5 + 0.38*(1 + min((wobb-90)/90,1) - max((wobb-90)/90,1)));
                wobb += 3;
                if wobb &gt;= 360
                {   wobb -= 360;
                    action++;
                }
                else if !((wobb+90)%180) and (irandom(100)&lt;20+2*action)
                {   timer1 = 5;
                    action = 0;
                }
                y += 9 * sign(room_height*(0.15+0.6*placement) - y);
                lgrip.y = y;
                if placement
                {   if y &gt;= room_height*(0.15+0.6*placement)
                        placement = 0;
                }
                else if y &lt;= room_height*(0.15+0.6*placement)
                    placement = 1;
            }
            else
            {   if timer2
                {   timer2 -= 1/room_speed;
                    hspeed += hspeed/12;
                    var hit = XY_COLLISION(side_wall,1);
                    if hit[0]
                    {   hspeed = 0;
                        vspeed = 0;
                        attack = 1;
                        repeat 6
                        with instance_create(irandom_range(room_width*0.05,room_width*0.95),
                                             room_height*-0.2,waste)
                        {   image_xscale = 3;
                            image_yscale = 3;
                            gravity = 0.05 + random(0.2);
                            shot = 0;
                            with instance_create(x,0,warning)
                            {   timer = 2;
                                image_blend = c_green;
                            }
                        }
                    }
                    if !timer2
                    {   attack--;
                        /*with instance_create(x,y,bullet)
                            motion_add(other.a_shoot,80);
                        if attack
                            timer2 = 0.7;
                        else
                            action = 0;*/
                    }
                }
                else if instance_exists(machine.me)
                {   timer1 -= 1/room_speed;
                    if !timer1
                    {   timer2 = 10;
                        hspeed = 1 * sign(machine.me.x-x);
                        if hspeed == 0
                            hspeed = 1;
                    }
                    else
                    {   vspeed = 12 * sign(machine.me.y-y);
                        lgrip.y = y;
                    }
                }
            }
            break;
}
    
</string>
          </argument>
        </arguments>
      </action>
    </event>
