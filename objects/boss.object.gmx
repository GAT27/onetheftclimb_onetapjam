        <arguments>
          <argument>
            <kind>1</kind>
            <string>///LIMBS

draw_self();
if machine.game_boss &gt;= 2
{   draw_line_width_color(x,y,lgrip.x+128,lgrip.y,10,c_olive,c_olive);
    draw_line_width_color(x,y+64,lgrip.x+128,lgrip.y+128,20,c_olive,c_olive);
    draw_line_width_color(x,y-64,lgrip.x+128,lgrip.y-128,20,c_olive,c_olive);
    if machine.game_boss == 2
    {   draw_line_width_color(x,y,rgrip.x-128,rgrip.y,10,c_olive,c_maroon);
        draw_line_width_color(x,y+64,rgrip.x-128,rgrip.y+128,20,c_olive,c_maroon);
        draw_line_width_color(x,y-64,rgrip.x-128,rgrip.y-128,20,c_olive,c_maroon);
    }
    if wobb
    {   //var v = 20*(1 + min((a_angle-90)/90,1) - max((a_angle-90)/90,1));
        //var a = point_direction(x,y,machine.me.x,machine.me.y);
        //var d = point_distance(x,y,machine.me.x,machine.me.y);
        draw_line_width_color(x,y,x+dcos(a_shoot)*10000,y-dsin(a_shoot)*10000,5+35*!!timer1,c_maroon,c_maroon);
    }
}

</string>
          </argument>
        </arguments>
      </action>
